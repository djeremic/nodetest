<div class="map-wr" style="padding-top: 50px;">
    <h1>PERSONALIZE</h1>
    <p>add your photo</p>
</div>

{{#if error}}
    <div class="alert alert-danger">
        <p>{{error}}</p>
    </div>
{{/if}}

<div class="row" style="margin-top:50px;text-align: center;">
    <form action="/map/upload" method="post" enctype="multipart/form-data" style="background:#fff;padding:20px;text-align: center; margin-bottom:50px;">
        <input type="hidden" name="id" value="{{id}}" />
        <input type="file" name="photo" style="text-align: center"/>
        <button class="btn btn-lg form-btn">Next</button>
    </form>


</div>