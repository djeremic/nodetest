<h1>List of places</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Address</th>
        </tr>
    </thead>
    <tbody>
        {{#each places}}
            <tr>
                <td>{{name}}</td>
                <td>{{address}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>

<a href="/places/add" class="btn btn-default">add new place</a><br/>